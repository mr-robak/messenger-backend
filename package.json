{
  "name": "messenger-backend",
  "version": "1.0.0",
  "description": "Backend for React Native messenger app.",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "dev": "npx nodemon .",
    "initdev": "npm run migrate && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "createDB": "npx sequelize-cli db:create",
    "dropDB": "npx sequelize-cli db:drop",
    "resetDB": "npx sequelize-cli db:migrate:undo:all && npm run initdev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Marcin Robak",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.15.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.2.0",
    "pg": "^8.2.1",
    "sequelize": "^6.2.4",
    "sequelize-cli": "^6.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
